<h1 style="text-align:center">My Items</h1>


<div class="main-container" id="index-items">
  <div class="container" id="cards-container">
    <div class="cards">
    <% @items.each do |item|%>
     <div class="card">
      <%= cl_image_tag item.photos.first.key if item.photos.first %>
      <h4><%= item.name %></h4>
      </div>
    <% end %>
  </div>
</div>
</div>






